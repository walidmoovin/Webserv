{
	"max_clients": 30,
	"root_folder": "./www",
	"servers": [
		{
			"server_name": "localhost",
			"listens": ["80", "localhost:80", "localhost", "[::]:443"],
			"return": "301 https://$host$uri"
		},
		{
			"server_name": "localhost",
			"listens": ["8080"],
			"root": "./public",
			"index": ["index.html"],
			"locations":
			{
				"/docs": 
				{
					"root": "./documents",
					"autoindex": true
				},
				"/img": 
				{
					"root": "./images"
				}

			}
		}
	]
}
